<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ite.financeiroandroid" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="br.edu.ite.financeiroandroid.MainActivity" android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="br.edu.ite.financeiroandroid.ListaPessoasActivity"></activity>
        <activity android:name="br.edu.ite.financeiroandroid.CadastroPessoaActivity"></activity>
        <activity android:name="br.edu.ite.financeiroandroid.ListaLancamentosActivity"></activity>
        <activity android:name="br.edu.ite.financeiroandroid.CadastroLancamentoActivity"></activity>
    </application>

</manifest>
